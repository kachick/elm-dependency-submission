{
  "name": "elm-dependency-submission",
  "version": "1.0.0",
  "description": "",
  "main": "lib/main.js",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "lint:fix": "eslint --fix src/**/*.ts *.ts",
    "lint:check": "eslint src/**/*.ts *.ts",
    "prepackage": "tsx scripts/rmrf.ts lib dist",
    "package": "esbuild src/main.ts --bundle --platform=node --target=node16 --packages=external --outfile=lib/main.js && ncc build",
    "postpackage": "tsx scripts/rmrf.ts lib",
    "test": "jest",
    "typecheck": "tsc",
    "repl": "tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kachick/elm-dependency-submission.git"
  },
  "keywords": [],
