{
  "name": "mobu-elm",
  "private": true,
  "version": "0.0.1",
  "description": "mobu timer",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "serve dist -l 8938 -n",
    "test:server": "run-p 'dev -- --port 8936' serve",
    "preview": "vite preview",
    "prettier-check": "prettier --list-different \"src/**/*.{js,ts,tsx,css,scss,html}\" \"./*.html\"",
    "prettier:fix": "prettier --write \"src/**/*.{js,ts,tsx,css,scss,html}\" \"./*.html\"",
    "eslint": "eslint --ignore-pattern \"src/**/*d.ts\" \"src/**/*.{js,ts,tsx}\"",
    "eslint:fix": "eslint --ignore-pattern \"src/**/*d.ts\" --fix \"src/**/*.{js,ts,tsx}\""
  },
  "author": "Kenichi Kamiya <kachick1@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "elm-review": "^2.7.5",
    "elm-test": "^0.19.1-revision9",
    "eslint": "^8.23.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "vite": "^3.0.9",
    "vite-plugin-elm": "^2.7.2"
  }
}
